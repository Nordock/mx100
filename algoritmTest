1. 
for(x=1; x<=10; x++) 
{ 
    for(z=10; z>=x; z–)
       print: ” “;
    for(z=x;z<2*x;z++)
    {
        y=z%10;
        print:y; 
    } 
    for(z=2*(x-1);z>=x; z–)
    {
        y = z%10;
        print: y;
    }
    Print: “\n”;
}


